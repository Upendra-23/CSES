#include<bits/stdc++.h>
using namespace std;

int main() {
	int n;
	cin >> n;
	int even = n;
	if (n & 1) even = n - 1;
	if (even - 1 == 1) {
		cout << "NO SOLUTION";
		return 0;
	}
	vector<int> ans;
	for (int i = 2; i <= n; i += 2) cout << i << " ";
	for (int i = 1; i <= n; i += 2) cout << i << " ";
}
